<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/resumeCarFragment">

    <fragment
        android:id="@+id/resumeCarFragment"
        android:name="com.example.test2.ui.car.resumeCars.ResumeCarFragment"
        android:label="fragment_resume_car"
        tools:layout="@layout/fragment_resume_car" >
        <action
            android:id="@+id/action_resumeCarFragment_to_newCarFragment"
            app:destination="@id/newCarFragment" />
        <action
            android:id="@+id/action_resumeCarFragment_to_detailCarFragment"
            app:destination="@id/detailCarFragment" />
    </fragment>
    <fragment
        android:id="@+id/newCarFragment"
        android:name="com.example.test2.ui.car.newCar.NewCarFragment"
        android:label="fragment_new_car"
        tools:layout="@layout/fragment_new_car" />
    <fragment
        android:id="@+id/detailCarFragment"
        android:name="com.example.test2.ui.car.DetailCarFragment"
        android:label="fragment_detail_car"
        tools:layout="@layout/fragment_detail_car" />
</navigation>